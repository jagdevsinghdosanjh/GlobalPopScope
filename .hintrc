{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 132",
    "not firefox <= 135"
  ]
}